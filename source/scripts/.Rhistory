Check_File <- function(path, name){
error <- FALSE
log <- ""
info <- ""
# load file content
con <- file(info$paths[i])
top <- readLines(con , n = 10)
close(con)
# check after regex
if( any(!grepl("^.*\t[0-9]*$", top[-1], perl = TRUE)) ){
error <- TRUE
log <- c(log, paste(userID, ": lines 2-9 have exception to regex: ^.*\t[0-9]*$"))
info <- paste0(info,paste("Content of extracted/ mapped", name, "does not look like a readcount file.<br/>"))
}
return(error, log, info)
}
